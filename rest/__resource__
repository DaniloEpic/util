<?phprequire "../lib/RestResource.php";require "../lib/RestRequest.php";class @resResource extends RestResource {private static $instance; public static function getInstance() {  if (!isset(self::$instance)) {  self::$instance = new @resResource();  } return self::$instance; } private function @resResource() { // } public function if_requested($v) { $r = @resResource::getInstance(); $a = $r->getRequest($v);  if (!$a) {  bad_request_header();  } return $a; } public function doGet() { $r = @resResource::getInstance(); // }}$r = @resResource::getInstance();$r->setRequest();$r->init();?>